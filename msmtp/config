port 587
from gao.dean@hotmail.com
user gao.dean@hotmail.com
passwordeval "pass gao.dean@hotmail.com"
auth on
tls on
tls_trust_file	/etc/ssl/cert.pem
logfile /Users/deangao/.cache/msmtp/msmtp.log
# tls_starttls

